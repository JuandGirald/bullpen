<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(project_interest_point) -%>">
  <span class='title'>
    <%= project_interest_point.content %>

  </span>
  <span class='actions'>
    
    <%= link_to refinery_icon_tag("application_go.png"), refinery.cvs_project_interest_point_path(project_interest_point),
        :title => t('.view_live_html'),
        :target => "_blank" %>
    
    <%= link_to refinery_icon_tag("application_edit.png"), refinery.edit_cvs_admin_project_interest_point_path(project_interest_point),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), refinery.cvs_admin_project_interest_point_path(project_interest_point),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :data => {:confirm => t('message', :scope => 'refinery.admin.delete', :title => project_interest_point.content)},
        :method => :delete %>
  </span>
</li>
