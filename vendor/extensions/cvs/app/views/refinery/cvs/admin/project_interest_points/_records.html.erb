<%= render 'refinery/admin/search_header', :url => refinery.cvs_admin_project_interest_points_path %>
<div class='pagination_container'>
  <% if @project_interest_points.any? %>
    <%= render 'project_interest_points' %>
  <% else %>
    <p>
      <% unless searching? %>
        <strong>
          <%= t('.no_items_yet') %>
        </strong>
      <% else %>
        <%= t('no_results', :scope => 'refinery.admin.search') %>
      <% end %>
    </p>
  <% end %>
</div>
